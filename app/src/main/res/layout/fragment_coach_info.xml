<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ui.settings.coach.CoachInfoFragment">

    <data></data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <com.google.android.material.textview.MaterialTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="1 out of 1"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="40dp"
            android:layout_marginEnd="16dp"
            />

        <com.google.android.material.textview.MaterialTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Experience"
            android:textStyle="bold"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="120dp"
            android:layout_marginStart="60dp"
            android:id="@+id/experience_textview"/>
        <com.google.android.material.textfield.TextInputLayout
            android:layout_width="176dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="30dp"
            app:layout_constraintTop_toTopOf="@id/experience_textview"
            app:layout_constraintStart_toEndOf="@id/experience_textview"
            app:layout_constraintBottom_toBottomOf="@id/experience_textview"
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
            android:id="@+id/experience_input_layout">
            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="176dp"
                android:layout_height="wrap_content"
                android:inputType="text"
                android:id="@+id/experience_edit_text"/>
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textview.MaterialTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Email"
            android:textStyle="bold"
            android:textSize="16sp"
            android:layout_marginTop="50dp"
            app:layout_constraintStart_toStartOf="@id/experience_textview"
            app:layout_constraintTop_toBottomOf="@id/experience_textview"
            android:id="@+id/email_textview"/>
        <com.google.android.material.textfield.TextInputLayout
            android:layout_width="176dp"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@id/email_textview"
            app:layout_constraintStart_toStartOf="@id/experience_input_layout"
            app:layout_constraintBottom_toBottomOf="@id/email_textview"
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
            android:id="@+id/email_input_layout">
            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="176dp"
                android:layout_height="wrap_content"
                android:inputType="phone"
                android:id="@+id/email_edit_text"/>
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textview.MaterialTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Phone"
            android:textStyle="bold"
            android:textSize="16sp"
            android:layout_marginTop="50dp"
            app:layout_constraintStart_toStartOf="@id/email_textview"
            app:layout_constraintTop_toBottomOf="@id/email_textview"
            android:id="@+id/phone_textview"/>
        <com.google.android.material.textfield.TextInputLayout
            android:layout_width="176dp"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@id/phone_textview"
            app:layout_constraintStart_toStartOf="@id/email_input_layout"
            app:layout_constraintBottom_toBottomOf="@id/phone_textview"
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
            android:id="@+id/phone_input_layout">
            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="176dp"
                android:layout_height="wrap_content"
                android:inputType="phone"
                android:id="@+id/phone_edit_text"/>
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textview.MaterialTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Price Range"
            android:textStyle="bold"
            android:textSize="16sp"
            android:layout_marginTop="50dp"
            app:layout_constraintStart_toStartOf="@id/phone_textview"
            app:layout_constraintTop_toBottomOf="@id/phone_textview"
            android:id="@+id/price_range_textview"/>
        <com.google.android.material.textfield.TextInputLayout
            android:layout_width="176dp"
            android:layout_height="wrap_content"
            app:endIconDrawable="@drawable/money"
            app:endIconMode="custom"
            app:layout_constraintTop_toTopOf="@id/price_range_textview"
            app:layout_constraintStart_toStartOf="@id/phone_input_layout"
            app:layout_constraintBottom_toBottomOf="@id/price_range_textview"
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
            android:id="@+id/price_range_input_layout">
            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="176dp"
                android:layout_height="wrap_content"
                android:inputType="number"
                android:id="@+id/price_range_edit_text"/>
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.button.MaterialButton
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/price_range_input_layout"
            android:id="@+id/done_btn"
            android:text="Done"
            android:textSize="16sp"
            android:textAllCaps="false"
            android:layout_marginBottom="100dp"
            />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>